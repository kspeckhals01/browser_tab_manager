{
    "manifest_version": 3,
    "name": "Browser Tab Manager",
    "version": "0.1",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png"
        },
        "default_title": "Tab Manager"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [ "tabs" ],
    "host_permissions": [ "<all_urls>" ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
