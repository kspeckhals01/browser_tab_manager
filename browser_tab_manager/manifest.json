{
    "manifest_version": 3,
    "name": "Tabvana",
    "version": "0.1",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png"
        },
        "default_title": "Tabvana"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [ "tabs", "storage", "identity" ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [ "scripts/login-init.js", "login.html" ],
            "matches": [ "<all_urls>" ]
        }
    ]
}
